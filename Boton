void main(){
  animate(ejecutar);
}

void ejecutar(){
  imagen();
  control();
  }

void imagen(){
  imagen("",0,0,100,100);
    rect(120,120,80,80);
    
}


void control(){	
  println("x: " + mouseX);
  println("y: " + mouseY);
  if (mousePressed)
    if(mouseX>120 && mouseX <200)
    if(mouseY>120 && mouseY<200)
    if(mousePressed){
      if(mouseButton ==LEFTBUTTON){
        background(200,0,10);
        rect(120,120,80,80);
        }
  }
}
  
